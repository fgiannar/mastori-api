{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/prawnsalad/Nexmo-PHP-lib.git"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "graham-campbell/exceptions": "^9.0",
        "tymon/jwt-auth": "0.5.*",
        "heroicpixels/filterable": "dev-master",
        "barryvdh/laravel-cors": "^0.8.0",
        "guzzlehttp/guzzle": "^6.1",
        "darkaonline/l5-swagger": "~4.0",
        "prawnsalad/nexmo": "@dev",
        "trexology/pointable": "^1.0",
        "nexmo/client": "@beta",
        "nexmo/laravel": "1.0.0-beta3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
     "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
